{
    "name": "developer-devPHP/zf2-phinx-module",
    "description": "Integrates the Phinx database migration tool into a ZF2 application.",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2", "phinx", "module"
    ],
    "homepage": "https://github.com/developer-devPHP/zf2-phinx-module",
    "authors": [
        {
            "name": "Developer",
        }
    ],
    "autoload": {
        "psr-0": {
            "PhinxModule": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require": {
        "php": ">=5.3.0",
        "robmorgan/phinx": "*",
        "symfony/yaml": "2.*",
        "zendframework/zendframework": "2.*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "dev-master",
        "squizlabs/php_codesniffer": "dev-master"
    }
}
